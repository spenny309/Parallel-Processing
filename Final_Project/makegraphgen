######################################################################
#
# 'make'         : create program $(PROG)
# 'make clean'   : clean directory
#
######################################################################

# list your source/assembly files in SRCS
SRCS = graph-generator.c

CFLAGS = -I.
LDLIBS = -L.

PROG = graph-generator

.PHONY: default clean

default: $(PROG)

$(PROG): $(SRCS)
	gcc $(CFLAGS) $(LDLIBS) -o $@ $^ -lm -O3

$(PROG).o:
	gcc $(CFLAGS) -o $@ -c $^

clean:
	rm -f *~ $(DRIVER).o $(PROG) $(PROG).o $(PROGEX)
